# File managed by Puppet
<% $settings.each |$k,$v| { -%>
<% if $v =~ String { -%>
<%= $k %>='<%= $v %>'
<% } -%>
<% if $v =~ Array { -%>
<%= $k %>='<% $v.each |$v2| { -%><%= $v2 %> <% } %>'
<% } -%>
<% if $v =~ Hash { -%>
<% $v.each |$k2,$v2| { -%>
<%= $k %>_<%= $k2 %>='<%= $v2 %>'
<% } -%>
<% } -%>
<% } -%>
